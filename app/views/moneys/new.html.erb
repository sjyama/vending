<%#= form_for @money do |f| %>
<!--上記記述の場合、undefined method `money_index_path' のエラーが発生-->
<%#= form_for @money, url: new_money_path, html: { method: :get} do |f| %>
<!--上記記述の場合、createメソッドが実行されない?状況だった。getをpostにしてもダメ。-->
<%= form_for @money, :url => {:action => :create} do |f| %>
  <h1>Money登録画面</h1>
  <p>お金の名前：<%= f.text_field :name %></p>
  <p>金額：<%= f.text_field :price %></p>
  <input type="submit" value="保存" >
<% end %>